[
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "方田",
        "text": "方田術曰廣從步數相乘得積步以畝法二百四十步除之即畝數百畝為一頃",
        "procedure_index": 0,
        "id": "九章算術_1_方田",
        "type": "field area",
        "text_punctuated": "方田術曰：廣從步數相乘得積步。以畝法二百四十步除之，即畝數。百畝為一頃。"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "里田",
        "text": "里田術曰廣從里數相乘得積里以三百七十五乘之即畝數",
        "procedure_index": 1,
        "id": "九章算術_1_里田",
        "type": "field area",
        "text_punctuated": "里田術曰：廣從里數相乘得積里。以三百七十五乘之，即畝數。"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "約分",
        "text": "約分術曰可半者半之不可半者副置分母子之數以少減多更相減損求其等也以等數約之",
        "procedure_index": 2,
        "id": "九章算術_1_約分",
        "type": "elementary fraction",
        "text_punctuated": "約分術曰：可半者半之；不可半者，副置分母、子之數，以少減多，更相減損，求其等也。以等數約之。"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "合分",
        "text": "合分術曰母互乘子并以為實母相乘為法實如法而一不滿法者以法命之其母同者直相從之",
        "procedure_index": 3,
        "id": "九章算術_1_合分",
        "type": "elementary fraction",
        "text_punctuated": "合分術曰：母互乘子，并以為實，母相乘為法。實如法而一。不滿法者，以法命之。其母同者，直相從之。"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "減分",
        "text": "減分術曰母互乘子以少減多餘為實母相乘為法實如法而一",
        "procedure_index": 4,
        "id": "九章算術_1_減分",
        "type": "elementary fraction",
        "text_punctuated": "減分術曰：母互乘子，以少減多，餘為實。母相乘為法。實如法而一。"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "課分",
        "text": "課分術曰母互乘子以少減多餘為實母相乘為法實如法而一即相多也",
        "procedure_index": 5,
        "id": "九章算術_1_課分",
        "type": "elementary fraction",
        "text_punctuated": "課分術曰：母互乘子，以少減多，餘為實。母相乘為法。實如法而一，即相多也。"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "平分",
        "text": "平分術曰母互乘子副并為平實母相乘為法以列數乘未并者各自為列實亦以列數乘法以平實減列實餘約之為所減并所減以益於少以法命平實各得其平",
        "procedure_index": 6,
        "id": "九章算術_1_平分",
        "type": "elementary fraction",
        "text_punctuated": "平分術曰：母互乘子，副并為平實。母相乘為法。以列數乘未并者各自為列實。亦以列數乘法。以平實減列實，餘，約之為所減。并所減以益於少。以法命平實，各得其平。"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "經分",
        "text": "經分術曰以人數為法錢數為實實如法而一有分者通之重有分者同而通之",
        "procedure_index": 7,
        "id": "九章算術_1_經分",
        "type": "elementary fraction",
        "text_punctuated": "經分術曰：以人數為法，錢數為實，實如法而一。有分者通之；重有分者同而通之。"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "乘分",
        "text": "乘分術曰母相乘為法子相乘為實實如法而一",
        "procedure_index": 8,
        "id": "九章算術_1_乘分",
        "type": "elementary fraction",
        "text_punctuated": "乘分術曰：母相乘為法，子相乘為實，實如法而一。"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "大廣田",
        "text": "大廣田術曰分母各乘其全分子從之相乘為實分母相乘為法實如法而一",
        "procedure_index": 9,
        "id": "九章算術_1_#9",
        "type": "elementary fraction",
        "text_punctuated": "大廣田術曰：分母各乘其全，分子從之，相乘為實。分母相乘為法。實如法而一。"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "#10",
        "text": "術曰半廣以乘正從",
        "procedure_index": 10,
        "id": "九章算術_1_#10",
        "type": "field area",
        "text_punctuated": "術曰：半廣以乘正從。"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "#11",
        "text": "術曰并兩邪而半之以乘正從若廣又可半正從若廣以乘并畝法而一",
        "procedure_index": 11,
        "id": "九章算術_1_#11",
        "type": "field area",
        "text_punctuated": "術曰：并兩邪而半之，以乘正從若廣。又可半正從若廣，以乘并。畝法而一。"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "#12",
        "text": "術曰并踵舌而半之以乘正從畝法而一",
        "procedure_index": 12,
        "id": "九章算術_1_#12",
        "type": "field area",
        "text_punctuated": "術曰：并踵、舌而半之，以乘正從。畝法而一。"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "#13",
        "text": "術曰半周半徑相乘得積步",
        "procedure_index": 13,
        "id": "九章算術_1_#13",
        "type": "field area",
        "text_punctuated": "術曰：半周半徑相乘得積步。"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "#14",
        "text": "術曰以徑乘周四而一",
        "procedure_index": 14,
        "id": "九章算術_1_#14",
        "type": "field area",
        "text_punctuated": "術曰：以徑乘周，四而一。"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "#15",
        "text": "術曰以弦乘矢矢又自乘并之二而一",
        "procedure_index": 15,
        "id": "九章算術_1_#15",
        "type": "field area",
        "text_punctuated": "術曰：以弦乘矢，矢又自乘，并之，二而一。"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "#16",
        "text": "術曰并中外周而半之以徑乘之為積步",
        "procedure_index": 16,
        "id": "九章算術_1_#16",
        "type": "field area",
        "text_punctuated": "術曰：并中外周而半之，以徑乘之為積步。"
    }
]