[
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "方田",
        "text": "方田術曰廣從步數相乘得積步以畝法二百四十步除之卽畝數百畝爲一頃",
        "procedure_index": 0,
        "id": "九章算術_1_方田",
        "type": "field area"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "里田",
        "text": "里田術曰廣從里數相乘得積里以三百七十五乘之卽畝數",
        "procedure_index": 1,
        "id": "九章算術_1_里田",
        "type": "field area"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "約分",
        "text": "約分術曰可半者半之不可半者副置分母子之數以少減多更相減損求其等也以等數約之",
        "procedure_index": 2,
        "id": "九章算術_1_約分",
        "type": "elementary fraction"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "合分",
        "text": "合分術曰母互乘子并以爲實母相乘爲法實如法而一不滿法者以法命之其母同者直相從之",
        "procedure_index": 3,
        "id": "九章算術_1_合分",
        "type": "elementary fraction"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "減分",
        "text": "減分術曰母互乘子以少減多餘爲實母相乘爲法實如法而一",
        "procedure_index": 4,
        "id": "九章算術_1_減分",
        "type": "elementary fraction"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "課分",
        "text": "課分術曰母互乘子以少減多餘爲實母相乗爲法實如法而一卽相多也",
        "procedure_index": 5,
        "id": "九章算術_1_課分",
        "type": "elementary fraction"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "平分",
        "text": "平分術曰母互乘子副并爲平實母相乘爲法以列數乘未并者各自爲列實亦以列數乘法以平實減列實餘約之爲所減并所減以益於少以法命平實各得其平",
        "procedure_index": 6,
        "id": "九章算術_1_平分",
        "type": "elementary fraction"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "經分",
        "text": "經分術曰以人數爲法錢數爲實實如法而一有分者通之重有分者同而通之",
        "procedure_index": 7,
        "id": "九章算術_1_經分",
        "type": "elementary fraction"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "乘分",
        "text": "乘分術曰母相乘爲法子相乘爲實實如法而一",
        "procedure_index": 8,
        "id": "九章算術_1_乘分",
        "type": "elementary fraction"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "大廣田",
        "text": "大廣田術曰分母各乘其全分子從之相乘爲實分母相乘爲法實如法而一",
        "procedure_index": 9,
        "id": "九章算術_1_#9",
        "type": "elementary fraction"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "#10",
        "text": "術曰半廣以乘正從",
        "procedure_index": 10,
        "id": "九章算術_1_#10",
        "type": "field area"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "#11",
        "text": "術曰并兩邪而半之以乘正從若廣又可半正從若廣以乘并畝法而一",
        "procedure_index": 11,
        "id": "九章算術_1_#11",
        "type": "field area"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "#12",
        "text": "術曰并踵舌而半之以乘正從畝法而一",
        "procedure_index": 12,
        "id": "九章算術_1_#12",
        "type": "field area"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "#13",
        "text": "術曰半周半徑相乘得積步",
        "procedure_index": 13,
        "id": "九章算術_1_#13",
        "type": "field area"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "#14",
        "text": "術曰以徑乘周四而一",
        "procedure_index": 14,
        "id": "九章算術_1_#14",
        "type": "field area"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "#15",
        "text": "術曰以弦乘矢矢又自乘并之二而一",
        "procedure_index": 15,
        "id": "九章算術_1_#15",
        "type": "field area"
    },
    {
        "source_title": "九章算術",
        "source_juan": 1,
        "name": "#16",
        "text": "術曰并中外周而半之以徑乘之爲積步",
        "procedure_index": 16,
        "id": "九章算術_1_#16",
        "type": "field area"
    }
]
