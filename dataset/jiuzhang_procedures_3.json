[
    {
        "source_title": "九章算術",
        "source_juan": 3,
        "name": "#0",
        "text": "術曰列置爵數各自爲衰副并爲法以五鹿乘未并者各自爲實實如法得一鹿",
        "procedure_index": 0,
        "id": "九章算術_3_#0",
        "type": "decreasing shares"
        
    },
    {
        "source_title": "九章算術",
        "source_juan": 3,
        "name": "#1",
        "text": "術曰置牛四馬二羊一各自爲列衰副并爲法以五斗乘未并者各自爲實實如法得一斗",
        "procedure_index": 1,
        "id": "九章算術_3_#1",
        "type": "decreasing shares"
    },
    {
        "source_title": "九章算術",
        "source_juan": 3,
        "name": "#2",
        "text": "術曰各置錢數爲列衰副并爲法以百錢乘未并者各自爲實實如法得一錢",
        "procedure_index": 2,
        "id": "九章算術_3_#2",
        "type": "decreasing shares"
    },
    {
        "source_title": "九章算術",
        "source_juan": 3,
        "name": "#3",
        "text": "術曰置一二四八十六爲列衰副并爲法以五尺乘未并者各自爲實實如法得一尺",
        "procedure_index": 3,
        "id": "九章算術_3_#3",
        "type": "decreasing shares"
    },
    {
        "source_title": "九章算術",
        "source_juan": 3,
        "name": "#4",
        "text": "術曰各置算數爲列衰副并爲法以所發傜人數乘未并者各自爲實實如法得一人",
        "procedure_index": 4,
        "id": "九章算術_3_#4",
        "type": "decreasing shares"
    },
    {
        "source_title": "九章算術",
        "source_juan": 3,
        "name": "#5",
        "text": "術曰各置所禀粟斛斗數爵次均之以爲列衰副并而加後來大夫亦五斗得二十以爲法以五斗乘未并者各自爲實實如法得一斗",
        "procedure_index": 5,
        "id": "九章算術_3_#5",
        "type": "decreasing shares"
    },
    {
        "source_title": "九章算術",
        "source_juan": 3,
        "name": "#6",
        "text": "術曰置三人人三二人人二爲列衰副并爲法以五斛乘未并者各自爲實實如法得一斛",
        "procedure_index": 6,
        "id": "九章算術_3_#6",
        "type": "decreasing shares"
    },
    {
        "source_title": "九章算術",
        "source_juan": 3,
        "name": "#7",
        "text": "術曰置爵數各自爲衰而返衰之副并爲法以百錢乘未并者各自爲實實如法得一錢",
        "procedure_index": 7,
        "id": "九章算術_3_#7",
        "type": "decreasing shares"
    },
    {
        "source_title": "九章算術",
        "source_juan": 3,
        "name": "#8",
        "text": "術曰以一斤價數爲法以一斤乘今有錢數爲實實如法得絲數",
        "procedure_index": 8,
        "id": "九章算術_3_#8",
        "type": "ratios"
    },
    {
        "source_title": "九章算術",
        "source_juan": 3,
        "name": "#9",
        "text": "術曰以一斤銖數爲法以一斤價數乘七兩一十二銖爲實實如法得錢數",
        "procedure_index": 9,
        "id": "九章算術_3_#9",
        "type": "ratios"
    },
    {
        "source_title": "九章算術",
        "source_juan": 3,
        "name": "#10",
        "text": "術曰以一丈寸數爲法以價錢數乘今有縑寸數爲實實如法得錢數",
        "procedure_index": 10,
        "id": "九章算術_3_#10",
        "type": "ratios"
    },
    {
        "source_title": "九章算術",
        "source_juan": 3,
        "name": "#11",
        "text": "術曰以一匹尺數爲法今有布尺數乘價錢爲實實如法得錢數",
        "procedure_index": 11,
        "id": "九章算術_3_#11",
        "type": "ratios"
    },
    {
        "source_title": "九章算術",
        "source_juan": 3,
        "name": "#12",
        "text": "術曰以一十四斤兩數爲法以一十斤乘今有絲兩數爲實實如法得縑數",
        "procedure_index": 12,
        "id": "九章算術_3_#12",
        "type": "ratios"
    },
    {
        "source_title": "九章算術",
        "source_juan": 3,
        "name": "#13",
        "text": "術曰以一斤展十六兩爲法以七兩乘今有絲兩數爲實實如法得耗數",
        "procedure_index": 13,
        "id": "九章算術_3_#13",
        "type": "ratios"
    },
    {
        "source_title": "九章算術",
        "source_juan": 3,
        "name": "#14",
        "text": "術曰置生絲兩數除耗數餘以爲法三十斤乘乾絲兩數爲實實如法得生絲數",
        "procedure_index": 14,
        "id": "九章算術_3_#14",
        "type": "ratios"
    },
    {
        "source_title": "九章算術",
        "source_juan": 3,
        "name": "#15",
        "text": "術曰以畝二百四十步爲法以六升太半升乘今有田積步爲實實如法得粟數",
        "procedure_index": 15,
        "id": "九章算術_3_#15",
        "type": "ratios"
    },
    {
        "source_title": "九章算術",
        "source_juan": 3,
        "name": "#16",
        "text": "術曰以價錢爲法以一歲三百五十四日乘先取錢數爲實實如法得日數",
        "procedure_index": 16,
        "id": "九章算術_3_#16",
        "type": "ratios"
    },
    {
        "source_title": "九章算術",
        "source_juan": 3,
        "name": "#17",
        "text": "術曰以月三十日乘千錢爲法以息三十乘今所貸錢數又以九日乘之爲實實如法得一錢",
        "procedure_index": 17,
        "id": "九章算術_3_#17",
        "type": "ratios"
    }
]
