[
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#0",
        "text": "術曰列置甲六乙五丙四丁三戊二各自為差副併為法以鹿數乘未併者各自為實實如法而一",
        "procedure_index": 0,
        "id": "張邱建算經_3_#0"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#1",
        "text": "術曰置斜逐步數以射步數增之自相乘以追之未及步數自相乘減之餘以開方除之所得以減斜逐步數餘為法以斜逐步數乘未及步數為實實如法而一",
        "procedure_index": 1,
        "id": "張邱建算經_3_#1"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#2",
        "text": "術曰垣髙自乘以減材長自乘餘以開方除之所得以減材餘即却行尺數",
        "procedure_index": 2,
        "id": "張邱建算經_3_#2"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#3",
        "text": "術曰併南北壁髙而半之以廣袤乘之為實實如斛法而一得斛數",
        "procedure_index": 3,
        "id": "張邱建算經_3_#3"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#4",
        "text": "術曰上下周相乘又各自乘併以高乘之以三十六而一所得為實實如斛法而一得斛數",
        "procedure_index": 4,
        "id": "張邱建算經_3_#4"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#5",
        "text": "術曰倍上袤下袤從之亦倍下袤上袤從之各以其廣乘之併以深乘之六而一所得為實實如斛法而一得斛數",
        "procedure_index": 5,
        "id": "張邱建算經_3_#5"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#6",
        "text": "術曰上下方相乘又各自相乘併以深乘之三而一所得為實實如斛法而一得斛數",
        "procedure_index": 6,
        "id": "張邱建算經_3_#6"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#7",
        "text": "術曰置粟積尺以倉廣袤相乘而一所得倍之減南壁高尺數餘為北壁高",
        "procedure_index": 7,
        "id": "張邱建算經_3_#7"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#8",
        "text": "術曰置粟積尺以三十六乘之以高而一所得以上周自相乘減之餘以上周尺數從而開方除之所得即下周",
        "procedure_index": 8,
        "id": "張邱建算經_3_#8"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#9",
        "text": "術曰置粟積尺以三乘之為實上下方相乘併又各自乘併以為法實如法而一",
        "procedure_index": 9,
        "id": "張邱建算經_3_#9"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#10",
        "text": "術曰置粟積尺以六乘之深而一所得倍上袤以上廣乘之又以下廣乘上袤併以減之餘以倍下廣上廣從之而一得下袤",
        "procedure_index": 10,
        "id": "張邱建算經_3_#10"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#11",
        "text": "術曰如方程",
        "procedure_index": 11,
        "id": "張邱建算經_3_#11",
        "referenced_procedure": "九章算術_8_方程"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#12",
        "text": "術曰大兄二中弟一小弟一合一百五十八疋大兄一中弟二小弟一合一百三十六疋大兄一中弟一小二合一百一十四疋如方程而求即得",
        "procedure_index": 12,
        "id": "張邱建算經_3_#12",
        "referenced_procedure": "九章算術_8_方程"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#13",
        "text": "術曰三甲二乙一丙錢三百四甲六乙三丙錢六百二甲二乙三丙錢三百如方程即得",
        "procedure_index": 13,
        "id": "張邱建算經_3_#13",
        "referenced_procedure": "九章算術_8_方程"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#14",
        "text": "術曰以四乘十錢又以七乘之五而一所得半之以十錢增之得甲錢數以十錢減之得乙錢數",
        "procedure_index": 14,
        "id": "張邱建算經_3_#14"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#15",
        "text": "術曰置今有僱錢數以行道里數乘之以本車乘數乘之為實以本僱錢數乘今有車數為法實如法得一",
        "procedure_index": 15,
        "id": "張邱建算經_3_#15"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#16",
        "text": "術曰置糲米之數求為粺米所得之數以乘今有惡粟為實以本粟為法實如法得一",
        "procedure_index": 16,
        "id": "張邱建算經_3_#16"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#17",
        "text": "術曰置糳米數求御米之數為法又置今御米數以本粟乘之為實實如法得一",
        "procedure_index": 17,
        "id": "張邱建算經_3_#17"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#18",
        "text": "術曰置人數為實車數為法而一得四人共重又置一於上方命之實餘返減法訖以四加十一方一得五人共重法餘即四人共重車數實餘即五人共重車數",
        "procedure_index": 18,
        "id": "張邱建算經_3_#18"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#19",
        "text": "術曰各列置本持錢數副併為法以利錢乘未併者各自為實實如法得一",
        "procedure_index": 19,
        "id": "張邱建算經_3_#19"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#20",
        "text": "術曰置甲乙丙分得之數副併為法以置車錢數乘未併者各自為實實如法得一",
        "procedure_index": 20,
        "id": "張邱建算經_3_#20"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#21",
        "text": "術曰置假令雀一十五隻燕十隻盈四銖於右行又置假令雀十二隻燕十三隻不足八銖於左行以盈不足維乘之併以為實併盈不足為法實如法得一",
        "procedure_index": 21,
        "id": "張邱建算經_3_#21"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#22",
        "text": "術曰置今造弓數以弓日數乘之又以成弓人數乘之為實以今有人數乘本有弓數為法實如法得一",
        "procedure_index": 22,
        "id": "張邱建算經_3_#22"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#23",
        "text": "術曰置城周里尺數三而一所得五之又置五以三乘之又自相乘以三自乘而一所得四之併上位即得凡數城若圓者置城周里尺數三而一所得五之又併一二三四凡得一十以六乘之倂之得凡數",
        "procedure_index": 23,
        "id": "張邱建算經_3_#23"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#24",
        "text": "術曰置粟積尺以三十六乘之為實以下周自乘為法實如法得一",
        "procedure_index": 24,
        "id": "張邱建算經_3_#24"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#25",
        "text": "術曰置歸作日數以與粟乘之各自為實以一歲三百五十四日為法實如法得一",
        "procedure_index": 25,
        "id": "張邱建算經_3_#25"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#26",
        "text": "術曰置米數為實以六升乘三十五日為法實如法得一",
        "procedure_index": 26,
        "id": "張邱建算經_3_#26"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#27",
        "text": "術曰置麫斛斗升數為實以人日食相乘為法實如法得一",
        "procedure_index": 27,
        "id": "張邱建算經_3_#27"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#28",
        "text": "術曰置二人三日所得錮銅斤兩銖通之作銖以二人三日相乘除之為一人一日之銖二十四而一還以一人一日所得兩銖通分内子復以一月三日乘一人積分所得復以銖分母三通之為法又以今錮銅斤兩通為銖以少半銖者三分之一以三通内一以六乘之為實實如法而一得人數不盡約之為分",
        "procedure_index": 28,
        "id": "張邱建算經_3_#28"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#29",
        "text": "術曰立方再自乘又以十六乘之九而一所得開立方除之徑得圓徑",
        "procedure_index": 29,
        "id": "張邱建算經_3_#29"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#30",
        "text": "術曰令徑再自乘九之十六而一開立方除之得立方",
        "procedure_index": 30,
        "id": "張邱建算經_3_#30"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#31",
        "text": "術曰以材方寸數再自乘以枚數而一所得開立方除之得枕方",
        "procedure_index": 31,
        "id": "張邱建算經_3_#31"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#32",
        "text": "術曰以本人數乘築訖日數為實以今有人數為法實如法得一",
        "procedure_index": 32,
        "id": "張邱建算經_3_#32"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#33",
        "text": "術曰置初利還錢以三乘之併第二還錢又以四乘之併第三還錢又以五乘之併第四還錢訖併餘錢為實以四轉得利倍數相乘得一百二十減一餘為法實如法得一",
        "procedure_index": 33,
        "id": "張邱建算經_3_#33"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#34",
        "text": "術曰以七人乘一月三十日又以五斛乘之為實以三人乘四日為法實如法而得一",
        "procedure_index": 34,
        "id": "張邱建算經_3_#34"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#35",
        "text": "術曰併一百一日息以乘百日而半之即得",
        "procedure_index": 35,
        "id": "張邱建算經_3_#35"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#36",
        "text": "術曰列置共杯人數於右方又共置共杯數於左方以人數互乘杯數併以為法令人數相乘以乘杯數為實實如法得一",
        "procedure_index": 36,
        "id": "張邱建算經_3_#36"
    },
    {
        "source_title": "張邱建算經",
        "source_juan": 3,
        "name": "#37",
        "text": "術曰雞翁每增四雞母每減七雞雛每益三即得此問若依上術推算難以通曉然較之諸本並同疑其從來脫漏闕文蓋流傳既久無可考證自漢唐以來雖甄鸞李淳風注釋未見詳辨今將算學教授并謝察微擬立術草剏新添入其術曰置錢一百在地以九為法除之得雞母之數不盡者返減下法為雞翁之數别列雞都數一百隻在地減去雞翁母數餘即雞雛得合前問若雞翁每增四雞母每減七雞雛每益三或雞翁每減四雞母每增七雞雛每損三即各得又答之數",
        "procedure_index": 37,
        "id": "張邱建算經_3_#37"
    }
]
