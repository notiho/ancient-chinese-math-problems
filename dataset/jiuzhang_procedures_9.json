[
    {
        "source_title": "九章算術",
        "source_juan": 9,
        "name": "句股",
        "text": "句股術曰句股各自乘并而開方除之卽弦又股自乘以減弦自乘其餘開方除之卽句又句自乘以減弦自乘其餘開方除之卽股",
        "procedure_index": 0,
        "id": "九章算術_9_句股",
        "type": "gougu"
    },
    {
        "source_title": "九章算術",
        "source_juan": 9,
        "name": "#1",
        "text": "術曰令徑二尺五寸自乘以七寸自乘減之其餘開方除之卽廣",
        "procedure_index": 1,
        "id": "九章算術_9_#1",
        "type": "gougu"
    },
    {
        "source_title": "九章算術",
        "source_juan": 9,
        "name": "#2",
        "text": "術曰以七周乘三圍爲股木長爲句爲之求弦弦者葛之長",
        "procedure_index": 2,
        "id": "九章算術_9_#2",
        "type": "gougu"
    },
    {
        "source_title": "九章算術",
        "source_juan": 9,
        "name": "#3",
        "text": "術曰半池方自乘以出水一尺自乘減之餘倍出水除之卽得水深加出水數得葭長",
        "procedure_index": 3,
        "id": "九章算術_9_#3",
        "type": "gougu"
    },
    {
        "source_title": "九章算術",
        "source_juan": 9,
        "name": "#4",
        "text": "術曰以去本自乘令如委數而一所得加委地數而半之卽索長",
        "procedure_index": 4,
        "id": "九章算術_9_#4",
        "type": "gougu"
    },
    {
        "source_title": "九章算術",
        "source_juan": 9,
        "name": "#5",
        "text": "術曰以垣高十尺自乘如郤行尺數而一所得以加郤行尺數而半之卽木長數",
        "procedure_index": 5,
        "id": "九章算術_9_#5",
        "type": "gougu"
    },
    {
        "source_title": "九章算術",
        "source_juan": 9,
        "name": "#6",
        "text": "術曰半鐻道自乘如深寸而一以深寸增之卽材徑",
        "procedure_index": 6,
        "id": "九章算術_9_#6",
        "type": "gougu"
    },
    {
        "source_title": "九章算術",
        "source_juan": 9,
        "name": "#7",
        "text": "術曰以去閫一尺自乘所得以不合二寸半之而一所得增不合之半卽得門廣",
        "procedure_index": 7,
        "id": "九章算術_9_#7",
        "type": "gougu"
    },
    {
        "source_title": "九章算術",
        "source_juan": 9,
        "name": "#8",
        "text": "術曰令一丈自乘爲實半相多令自乘倍之減實半其餘以開方除之所得減相多之半卽戸廣加相多之半卽戸高",
        "procedure_index": 8,
        "id": "九章算術_9_#8",
        "type": "gougu"
    },
    {
        "source_title": "九章算術",
        "source_juan": 9,
        "name": "#9",
        "text": "術曰從橫不出相乘倍而開方除之所得加從不出卽戸廣加橫不出卽戸高兩不出加之得戸袤",
        "procedure_index": 9,
        "id": "九章算術_9_#9",
        "type": "gougu"
    },
    {
        "source_title": "九章算術",
        "source_juan": 9,
        "name": "#10",
        "text": "術曰以去本自乘令如高而一所得以減竹高而半其餘卽折者之高也",
        "procedure_index": 10,
        "id": "九章算術_9_#10",
        "type": "gougu"
    },
    {
        "source_title": "九章算術",
        "source_juan": 9,
        "name": "#11",
        "text": "術曰令七自乘三亦自乘并而半之以爲甲邪行率邪行率減於七自乘餘爲南行率以三乘七爲乙東行率置南行十步以甲邪行率乘之副置十步以乙東行率乘之各自爲實實如南行率而一各得行數",
        "procedure_index": 11,
        "id": "九章算術_9_#11",
        "type": "gougu"
    },
    {
        "source_title": "九章算術",
        "source_juan": 9,
        "name": "#12",
        "text": "術曰并句股爲法句股相乘爲實實如法而一得方一步",
        "procedure_index": 12,
        "id": "九章算術_9_#12",
        "type": "gougu"
    },
    {
        "source_title": "九章算術",
        "source_juan": 9,
        "name": "#13",
        "text": "術曰八步爲句十五步爲股爲之求弦三位并之爲法以句乘股倍之爲實實如法得徑一步",
        "procedure_index": 13,
        "id": "九章算術_9_#13",
        "type": "gougu"
    },
    {
        "source_title": "九章算術",
        "source_juan": 9,
        "name": "#14",
        "text": "術曰出東門步數爲法半邑方自乘爲實實如法得一步",
        "procedure_index": 14,
        "id": "九章算術_9_#14",
        "type": "gougu"
    },
    {
        "source_title": "九章算術",
        "source_juan": 9,
        "name": "#15",
        "text": "術曰東門南至隅步數以乘南門東至隅步數爲實以木去門步數爲法實如法而一",
        "procedure_index": 15,
        "id": "九章算術_9_#15",
        "type": "gougu"
    },
    {
        "source_title": "九章算術",
        "source_juan": 9,
        "name": "#16",
        "text": "術曰令兩出門步數相乘因而四之爲實開方除之卽得邑方",
        "procedure_index": 16,
        "id": "九章算術_9_#16",
        "type": "gougu"
    },
    {
        "source_title": "九章算術",
        "source_juan": 9,
        "name": "#17",
        "text": "術曰以出北門步數乘西行步數倍之爲實并出南門步數爲從法開方除之卽邑方",
        "procedure_index": 17,
        "id": "九章算術_9_#17",
        "type": "gougu"
    },
    {
        "source_title": "九章算術",
        "source_juan": 9,
        "name": "#18",
        "text": "術曰令五自乘三亦自乘并而半之爲邪行率邪行率減於五自乘者餘爲南行率以三乘五爲乙東行率置邑方半之以南行率乘之如東行率而一卽得出南門步數以增邑方半卽南行置南行步求弦者以邪行率乘之求東者以東行率乘之各自爲實實如南行率得一步",
        "procedure_index": 18,
        "id": "九章算術_9_#18",
        "type": "gougu"
    },
    {
        "source_title": "九章算術",
        "source_juan": 9,
        "name": "#19",
        "text": "術曰令一丈自乘爲實以三寸爲法實如法而一",
        "procedure_index": 19,
        "id": "九章算術_9_#19",
        "type": "gougu"
    },
    {
        "source_title": "九章算術",
        "source_juan": 9,
        "name": "#20",
        "text": "術曰置木高減人目高七尺餘以乘五十三里爲實以人去木三里爲法實如法而一所得加木高卽山高",
        "procedure_index": 20,
        "id": "九章算術_9_#20",
        "type": "gougu"
    },
    {
        "source_title": "九章算術",
        "source_juan": 9,
        "name": "#21",
        "text": "術曰置井徑五尺以入徑四寸減之餘以乘立木五尺爲實以入徑四寸爲法實如法得一寸",
        "procedure_index": 21,
        "id": "九章算術_9_#21",
        "type": "gougu"
    }
]
